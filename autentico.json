  "authAccountLockoutMaxAttempts": 5,
  "authAccountLockoutDuration": "15m",
  "auth_mode": "password",
  "passkey_rp_name": "Autentico",
  "trust_device_enabled": false,
  "trust_device_expiration": "720h",
  "cleanup_interval": "6h",
