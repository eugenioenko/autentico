  "authCSRFProtectionSecretKey": "your-secret-here",
  "authCSRFSecureCookie": false,
  "authAllowedRedirectURIs": [],
  "authJwkCertKeyID": "autentico-key-1",
  "authPrivateKeyFile": "./db/private_key.pem",
  "swaggerPort": "8888",
